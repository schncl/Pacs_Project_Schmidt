{
    "dataset_file": "heat_dataset_sine_continuous_direct_params.mat",
    "input_fields": ["kx","ky"],
    "target_fields": ["deg_x","deg_y"],


    "neural_network": {
        "epochs": 5000,
        "batch_size": 32,
        "learning_rate": 0.001,
        "hidden_layers": [64, 128],
        "dropout_rate": 0.2
      },
    

    "gaussian_process": {
        "n_restarts_optimizer": 10,
        "random_state": 42,
        "constant_value": 1.0,
        "constant_bounds": [1e-3, 1e3],
        "rbf_length_scale": 1.0,
        "rbf_bounds": [1e-2, 1e2]
      }
    
}